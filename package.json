{
  "name": "onnxjs",
  "version": "0.1.6",
  "description": "A Javascript library for running ONNX models on browsers and on Node.js",
  "main": "./lib/api/index.js",
  "browser": "./dist/onnx.min.js",
  "types": "./types/lib/api/index.d.ts",
  "scripts": {
    "prepare": "tsc && node tools/build",
    "prepack": "node tools/build --build-wasm --build-bundle",
    "build": "tsc && node tools/build --build-wasm --build-bundle",
    "build:clean": "tsc && node tools/build --clean-install --build-wasm --build-bundle",
    "build:doc": "tsc && node tools/gen-doc",
    "build:node": "tsc",
    "build:wasm": "node tools/build --build-wasm",
    "build:bundle": "node tools/build --build-bundle",
    "test": "node tools/test-runner-cli",
    "lint": "tslint -p . -t verbose",
    "format": "clang-format --glob=\"{{lib,test,tools}/**/*.ts,src/**/*.{cpp,h}}\" --style=file -i",
    "format:md": "prettier --write --parser markdown \"{README.md,docs/**/*.md}\"",
    "format:jsonc": "prettier --write --parser json \"test/**/*.jsonc\"",
    "pre-commit": "node tools/pre-commit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/onnxjs.git"
  },
  "keywords": [
    "ONNX.js"
  ],
  "author": "fs-eire",
  "license": "MIT",
  "dependencies": {
    "onnx-proto": "^4.0.2",
    "platform": "^1.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.5",
    "@types/karma": "^3.0.2",
    "@types/long": "^4.0.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.6",
    "@types/npmlog": "^4.1.1",
    "@types/platform": "^1.3.2",
    "@types/rimraf": "^2.0.2",
    "@types/strip-json-comments": "0.0.30",
    "@types/webgl2": "0.0.4",
    "chai": "^4.2.0",
    "clang-format": "^1.2.4",
    "electron": "^4.1.4",
    "fs-extra": "^7.0.1",
    "globby": "^9.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^1.3.1",
    "karma": "^4.1.0",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-electron": "^6.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "long": "^4.0.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "mocha": "^6.1.4",
    "npmlog": "^4.1.2",
    "numpy-parser": "^1.2.3",
    "onnxjs-node": "^0.4.0-rev.1",
    "prettier": "1.17.0",
    "strip-json-comments": "^2.0.1",
    "ts-loader": "^5.4.3",
    "tslint": "^5.16.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "worker-loader": "^2.0.0"
  }
}
